{
    "name": "joshhanley/livewire-dusk-testbench",
    "description": "A Laravel Dusk testing helper for Livewire package development.",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Hanley",
            "email": "josh@joshhanley.com.au"
        }
    ],
    "require": {
        "php": "^8.1",
        "livewire/livewire": "^3.1|dev-master",
        "orchestra/testbench-dusk": "^8.0|^9.0",
        "psy/psysh": "^0.11.1|^0.12"
    },
    "require-dev": {
        "spatie/laravel-ray": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "LivewireDuskTestbench\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LivewireDuskTestbench\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
